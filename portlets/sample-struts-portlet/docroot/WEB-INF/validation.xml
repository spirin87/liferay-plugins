<?xml version="1.0"?>
<!DOCTYPE form-validation PUBLIC "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1.3//EN" "http://jakarta.apache.org/commons/dtds/validator_1_1_3.dtd">

<form-validation>
	<formset>
		<form name="unsubscribeForm">
			<field depends="required" property="lastName">
				<msg key="error.lastName.required" name="required" />
			</field>
			<field depends="required" property="emailAddress">
				<msg key="error.emailAddress.required" name="required" />
			</field>
		</form>
	</formset>
</form-validation>